{
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "object",
   "title": "The Root Schema",
   "required": [
     "codigo",
     "alternativo",
     "descricao"
   ],
   "properties": {
     "codigo": {
       "$id": "#/properties/codigo",
       "type": "integer",
       "title": "The Codigo Schema",
       "default": 0,
       "examples": [
         100
       ]
     },
     "alternativo": {
       "$id": "#/properties/alternativo",
       "type": "string",
       "title": "The Alternativo Schema",
       "default": "",
       "examples": [
         "Área geográfica texto alternativo"
       ],
       "pattern": "^(.*)$"
     },
     "descricao": {
       "$id": "#/properties/descricao",
       "type": "string",
       "title": "The Descricao Schema",
       "default": "",
       "examples": [
         "Descrição da área geográfica"
       ],
       "pattern": "^(.*)$"
     }
   }
}